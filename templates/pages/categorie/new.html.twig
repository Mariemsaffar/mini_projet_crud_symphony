{% extends "base.html.twig" %}

{% block title %}Ajouter une Categorie
{% endblock %}

{% block body %}
	<div class="container">

		<h1 class="mt-4">Ajouter une Categorie</h1>
		{% form_theme form 'bootstrap_5_layout.html.twig' %}

		{{ form_start(form) }}
        {{form(form)}}
        {{form_end(form)}}
        <a type="submit" id="categorie_submit" name="categorie[submit]" class="btn btn-primary mt-4" href={{path("cat")}}>Ajouter une categorie</a>
 


        </div>
{% endblock %}
